{
  "name": "Darkblue Twitter",
  "version": "1.0",
  "description": "A dark blue theme for Twitter.",
  "author": "Arc0re",
  //"permissions": ["http://twitter.com/*","https://twitter.com/*"],
  "content_scripts": [
  	{
  		"matches": ["https://www.twitter.com/*","http://www.twitter.com/*","https://twitter.com/*"],
  		"css": ["darkblue.css"]
  	}
  ],
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}
